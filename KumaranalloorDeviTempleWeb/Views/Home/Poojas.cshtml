@{
    ViewBag.Title = "Poojas | Kumaranalloor Devi Temple";
}
<!-- Banner Start-->
<section class="kdt-mis_banner" style="max-height: 450px; text-align: center; background-image: url('../../Content/Images/kdt-poojas-02.jpg');">
    <!-- poojas -->
    <h1 style="vertical-align:central; padding-top:150px; color:#ffb400; font-weight: normal; font-family: Ubuntu,'Open Sans', sans-serif; font-size:36px;">POOJAS</h1>
</section>

    <section class="wiwet-getstarted">
        <div class="container" data-uk-scrollspy="{cls:'uk-animation-scale-up', repeat: false}">
            <div class="wiwet-headline">
                <h3>DAILY POOJAS</h3>
            </div>
            <div class="row">
                <br />
                <div class="col-sm-12" style="text-align:center;">
                    <div class="kdt-content" style="text-align:center;">
                        <div class="kdt-table-01">
                            <div class="kdt-table-row-01">
                                The temple opens at 04:00 AM
                            </div>
                            <div class="kdt-table-row-01">
                                <div class="kdt-table-cell-01-time">
                                    04:00 AM :
                                </div>
                                <div class="kdt-table-cell-01-pooja">
                                    Opening
                                </div>
                            </div>
                            <div class="kdt-table-row-01">
                                <div class="kdt-table-cell-01-time">
                                    04:00AM to 04:15AM :
                                </div>
                                <div class="kdt-table-cell-01-pooja">
                                    Nirmalyam
                                </div>
                            </div>
                            <div class="kdt-table-row-01">
                                <div class="kdt-table-cell-01-time">
                                    04:30 AM :
                                </div>
                                <div class="kdt-table-cell-01-pooja">
                                    Abhishekam
                                </div>
                            </div>
                            <div class="kdt-table-row-01">
                                <div class="kdt-table-cell-01-time">
                                    05:45 AM :
                                </div>
                                <div class="kdt-table-cell-01-pooja">
                                    Usha Pooja
                                </div>
                            </div>
                            <div class="kdt-table-row-01">
                                <div class="kdt-table-cell-01-time">
                                    06:30 AM :
                                </div>
                                <div class="kdt-table-cell-01-pooja">
                                    Ethirettu Pooja
                                </div>
                            </div>
                            <div class="kdt-table-row-01">
                                <div class="kdt-table-cell-01-time">
                                    07:00 AM :
                                </div>
                                <div class="kdt-table-cell-01-pooja">
                                    Seeveli
                                </div>
                            </div>
                            <div class="kdt-table-row-01">
                                <div class="kdt-table-cell-01-time">
                                    08:00 AM :
                                </div>
                                <div class="kdt-table-cell-01-pooja">
                                    Pantheeradi
                                </div>
                            </div>
                            <div class="kdt-table-row-01">
                                <div class="kdt-table-cell-01-time">
                                    10:30 AM :
                                </div>
                                <div class="kdt-table-cell-01-pooja">
                                    Ucha Pooja and Navagam
                                </div>
                            </div>
                            <div class="kdt-table-row-01">
                                <div class="kdt-table-cell-01-time">
                                    11:15 AM :
                                </div>
                                <div class="kdt-table-cell-01-pooja">
                                    Ucha Seeveli
                                </div>
                            </div>
                            <div class="kdt-table-row-01">
                                <div class="kdt-table-cell-01-time">
                                    11:15 AM :
                                </div>
                                <div class="kdt-table-cell-01-pooja">
                                    Ucha Seeveli
                                </div>
                            </div>
                            <div class="kdt-table-row-01">
                                Temple will be closed between 11:30 AM and 05:00 PM and re-opens at 05:00 PM
                            </div>
                            <div class="kdt-table-row-01">
                                <div class="kdt-table-cell-01-time">
                                    05:00 PM :
                                </div>
                                <div class="kdt-table-cell-01-pooja">
                                    Re-opening
                                </div>
                            </div>
                            <div class="kdt-table-row-01">
                                <div class="kdt-table-cell-01-time">
                                    07:00 PM :
                                </div>
                                <div class="kdt-table-cell-01-pooja">
                                    Athazha Pooja
                                </div>
                            </div>
                            <div class="kdt-table-row-01">
                                <div class="kdt-table-cell-01-time">
                                    07:30 PM :
                                </div>
                                <div class="kdt-table-cell-01-pooja">
                                    Deeparadhana
                                </div>
                            </div>
                            <div class="kdt-table-row-01">
                                <div class="kdt-table-cell-01-time">
                                    07:45 PM :
                                </div>
                                <div class="kdt-table-cell-01-pooja">
                                    Athazha Seeveli
                                </div>
                            </div>
                            <div class="kdt-table-row-01">
                                <div class="kdt-table-cell-01-time">
                                    08:00 PM :
                                </div>
                                <div class="kdt-table-cell-01-pooja">
                                    Closing
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>


    <!-- vazhipadu -->
    <div class="row">
        <section class="wiwet-getstarted">
            <div class="container" data-uk-scrollspy="{cls:'uk-animation-scale-up', repeat: false}">
                <div class="wiwet-headline">
                    <h3>VAZHIPADU</h3>
                </div>
                <div class="row">
                    <div class="col-sm-12">
                        <br />
                        <div class="kdt-content">
                            <div class="kdt-table-01">
                                <div class="kdt-table-row-01" style="background-color:#f3de61; font-weight:normal; text-align:center;">
                                    <div class="kdt-table-cell-01-diety">
                                        Diety
                                    </div>
                                    <div class="kdt-table-cell-01-offering">
                                        Offering
                                    </div>
                                    <div class="kdt-table-cell-01-materials">
                                        Materials Required
                                    </div>
                                    <div class="kdt-table-cell-01-amount">
                                        Amount (Rs)
                                    </div>
                                    <div class="kdt-table-cell-01-remarks">
                                        Remarks
                                    </div>
                                    <div class="kdt-table-cell-01-orderonline">
                                        Order
                                    </div>
                                </div>
                            </div>
                            @foreach (var vazhipadu in Model)
                            {
                                <div class="kdt-table-01">
                                    <div class="kdt-table-row-01">
                                        <div class="kdt-table-cell-01-diety">
                                            @vazhipadu.diety_name
                                        </div>
                                        <div class="kdt-table-cell-01-offering">
                                            @vazhipadu.offering_name
                                        </div>
                                        <div class="kdt-table-cell-01-materials">
                                            @vazhipadu.materials_reqd
                                        </div>
                                        <div class="kdt-table-cell-01-amount">
                                            @vazhipadu.offering_amount
                                        </div>
                                        <div class="kdt-table-cell-01-remarks">
                                            @vazhipadu.remarks
                                        </div>
                                        <div class="kdt-table-cell-01-orderonline">
                                            <button disabled>Order Online</button>
                                        </div>
                                    </div>
                                </div>
                            }
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </div>
    </div>
</section>

